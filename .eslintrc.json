{
	"root": true,
	"env": {
		"browser": true,
		"es2022": true
	},
	"extends": ["@w0s/eslint-config"],
	"ignorePatterns": ["@types/", "customelements/*/dist/", "javascript/*/dist/"],
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-console": [
			"warn",
			{
				"allow": ["info", "error"]
			}
		]
	}
}
