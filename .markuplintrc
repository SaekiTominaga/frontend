{
	"extends": [
		"./node_modules/@w0s/markuplint-config/.markuplintrc"
	],
	"rules": {
		"no-empty-palpable-content": false,
		"character-reference": false
	},
	"nodeRules": [
		{
			"selector": "div",
			"rules": {
				"required-attr": false
			}
		},
		{
			"selector": "table, input, select, textarea",
			"rules": {
				"require-accessible-name": false
			}
		}
	]
}
